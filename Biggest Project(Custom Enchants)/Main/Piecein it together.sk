        set slot 1 of player's current inventory to purple stained glass pane with name " "
        set slot 2 of player's current inventory to Bottle o' Enchanting with name "&aXp-Finder &7Tier &7[&aI&7]"
        set slot 3 of player's current inventory to purple stained glass pane with name " "
        set slot 4 of player's current inventory to Bottle o' Enchanting with name "&aXp-Finder &7Tier &7[&aII&7]"
        set slot 5 of player's current inventory to purple stained glass pane with name " "
        set slot 6 of player's current inventory to Bottle o' Enchanting with name "&aXp-Finder &7Tier &7[&aIII&7]"
        set slot 7 of player's current inventory to purple stained glass pane with name " "
        set slot 8 of player's current inventory to purple stained glass pane with name " "

        set slot 9 of player's current inventory to black stained glass pane with name " "
        set slot 10 of player's current inventory to emerald with name "&dGem-Finder &7Tier &7[&dI&7]"
        set slot 11 of player's current inventory to black stained glass pane with name " "
        set slot 12 of player's current inventory to emerald with name "&dGem-Finder &7Tier &7[&dII&7]"
        set slot 13 of player's current inventory to black stained glass pane with name " "
        set slot 14 of player's current inventory to emerald with name "&dGem-Finder &7Tier &7[&dIII&7]"
        set slot 15 of player's current inventory to black stained glass pane with name " "
        set slot 16 of player's current inventory to emerald with name "&dGem-Finder &7Tier &7[&dIV&7]"
        set slot 17 of player's current inventory to black stained glass pane with name " "

on inventory click:
    if name of event-inventory is "&5&lEnchant GUI":
        cancel event
        if name of item is "&aXp-Finder &7Tier &7[&aI&7]":
            if lore of player's tool contains "&aXp-Finder &7Tier &7[&aI&7]":
                send "&cYou already have that level or above!"
                cancel event
            if lore of player's tool contains "&aXp-Finder &7Tier &7[&aII&7]":
                send "&cYou already have that level or above!"
                cancel event
            if lore of player's tool contains "&aXp-Finder &7Tier &7[&aIII&7]":
                send "&cYou already have the max level of Xp-Finder(For now)"
                cancel event
            if lore of player's tool contains "&aXp-Finder &7Tier &7[&c0&7]":
                if player's inventory contains 1 netherite block named "&0&lCompressed Netherite":
                    remove 1 netherite block with name "&0&lCompressed Netherite" from player's inventory
                    set line 1 of lore of player's tool to "&aXp-Finder &7Tier &7[&aI&7]"
            
            else:
                cancel event
                send "&cYou do not have enough &0&lCompressed Netherite"

        if name of item is "&aXp-Finder &7Tier &7[&aII&7]":
            if lore of player's tool contains "&aXp-Finder: &cNull":
                send "&cYou need to buy tier I before buying tier II"
                cancel event
            if lore of player's tool contains "&aXp-Finder &7Tier &7[&aII&7]":
                send "&cYou already have that level or above!"
                cancel event
            if lore of player's tool contains "&aXp-Finder &7Tier &7[&aIII&7]":
                send "&cYou already have the max level of Xp-Finder(For now)"
                cancel event
            if lore of player's tool contains "&aXp-Finder &7Tier &7[&aI&7]":
                if player's inventory contains 10 netherite block named "&0&lCompressed Netherite":
                    remove 10 netherite block with name "&0&lCompressed Netherite" from player's inventory
                    set line 1 of lore of player's tool to "&aXp-Finder &7Tier &7[&aII&7]"
            else:
                cancel event
                send "&cYou do not have enough &0&lCompressed Netherite"

        if name of item is "&aXp-Finder &7Tier &aXp-Finder: &cNull":
            if lore of player's tool contains "&aXp-Finder: &cNull":
                send "&cYou need to buy tier II before buying tier III"
                cancel event
            if lore of player's tool contains "&aXp-Finder &7Tier &7[&aIII&7]":
                send "&cYou already have the max level of Xp-Finder(For now)"
                cancel event
            if lore of player's tool contains "&aXp-Finder &7Tier &7[&aII&7]":
                if player's inventory contains 64 netherite block named "&0&lCompressed Netherite":
                    remove 64 netherite block with name "&0&lCompressed Netherite" from player's inventory
                    set line 1 of lore of player's tool to "&aXp-Finder &7Tier &7[&aIII&7]"
            else:
                cancel event
                send "&cYou do not have enough &0&lCompressed Netherite"

