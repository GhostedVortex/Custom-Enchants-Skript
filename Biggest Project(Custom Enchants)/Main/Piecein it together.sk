command /currencycheck:
    aliases: /cc
    trigger:
        send "[&5&lCurrency] &7&lChecker:%nl%&aXp&8: &a%{Xp_Amount.%player%}%%nl%&dGems&8: &d%{Gem_Amount.%player%}%"

command /PlayerPick:
    aliases: /pp, /PlayerPick, /Playerp
    trigger:
        give player unbreakable netherite pickaxe named "&e%player%'s &7Pickaxe" with lore "&aXp-Finder: &cNull%nl%&dGem-Finder: &cNull"

